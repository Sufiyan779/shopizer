pipeline{
    agent {label 'GRADLE'}
 stages{
    stage('pull from vcs') {
        steps{
            git url: 'https://github.com/Sufiyan779/shopizer.git',
            branch: 'SPRINT_1_DEV'
        }

    }
    stage('Gradle build'){
        steps{
            sh 'gradle clean build'
        }
    }
 }   
}