pipeline{
    agent {label 'OPENJDK-11-1'}
    triggers{
        pollscm('* * * * *')
    }
 stages{
    stage('pull from vcs') {
        steps{
            git url: 'https://github.com/Sufiyan779/shopizer.git',
            branch: 'SPRINT_1_DEV'
        }

    }
    stage('Gradle build'){
        steps{
            sh 'mvn package'
        }
    }
 }   
}